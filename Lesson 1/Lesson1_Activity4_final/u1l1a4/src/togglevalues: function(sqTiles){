},
and here is the code for snap function :- greenTiles.input.enableSnap(32,32, false, true);

addGreenTiles: function(){
greenTiles= this.add.sprite(250,500,'greenTile');
greenTiles.inputEnabled=true;
greenTiles.input.enableSnap(32,32, false, true);
greenTiles.input.enableDrag()
greenTiles.events.onDragStart.add(this.addGreenTiles, this);
this.game.physics.arcade.enable(greenTiles);
greenTiles.events.onDragStop.add(this.stopDrag,this);
greenTiles.value=1;
},